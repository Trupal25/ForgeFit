{
  "name": "forgefit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "populate-exercises": "ts-node --project scripts/tsconfig.json scripts/populate-exercises.ts",
    "test-exercises": "ts-node --project scripts/tsconfig.json scripts/test-populate-exercises.ts",
    "check-db": "ts-node --project scripts/tsconfig.json scripts/check-database.ts",
    "fetch-exercises": "ts-node --project scripts/tsconfig.json scripts/fetch-all-exercises.ts",
    "populate-complete-exercises": "ts-node --project scripts/tsconfig.json scripts/populate-complete-exercises.ts",
    "debug-connection": "ts-node --project scripts/tsconfig.json scripts/debug-connection.ts",
    "show-db-info": "ts-node --project scripts/tsconfig.json scripts/show-db-info.ts",
    "db:reset": "prisma migrate reset --force",
    "db:setup": "prisma migrate dev && npm run populate-exercises",
    "db:setup-complete": "prisma migrate dev && npm run fetch-exercises && npm run populate-complete-exercises"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@google/genai": "^1.6.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "formidable": "^3.5.4",
    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "prisma": "^5.22.0",
    "react": "^19.0.0",
    "react-circular-progressbar": "^2.2.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.4",
    "@tailwindcss/postcss": "^4",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
